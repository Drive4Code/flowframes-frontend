{
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.includeLanguages": {
    "plaintext": "javascript",
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },
  "files.associations": {
    "*.ts": "typescript",
    "*.tsx": "typescriptreact"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
},
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
},
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
}
}
